{
  "name": "responsive-loader",
  "version": "0.7.0",
  "description": "A webpack loader for responsive images",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build-test-jimp": "rm -rf test/jimp/build && webpack --config=./test/jimp/webpack.config.js",
    "build-test-sharp": "rm -rf test/sharp/build && webpack --config=./test/sharp/webpack.config.js",
    "lint": "eslint --ignore-path=.gitignore .",
    "test-jimp": "npm run build-test-jimp && tape './test/jimp/build/test.js' | tap-spec",
    "test-sharp": "npm run build-test-sharp && tape './test/sharp/build/test.js' | tap-spec",
    "test": "npm run test-jimp && npm run test-sharp"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herrstucki/responsive-loader.git"
  },
  "keywords": [
    "webpack",
    "responsive",
    "loader",
    "srcset"
  ],
  "author": "Jeremy Stucki <jeremy@interactivethings.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/herrstucki/responsive-loader/issues"
  },
  "homepage": "https://github.com/herrstucki/responsive-loader",
  "dependencies": {
    "d3-queue": "^3.0.2",
    "loader-utils": "^0.2.16"
  },
  "devDependencies": {
    "eslint": "^3.2.2",
    "eslint-config-interactivethings": "^3.0.0",
    "eslint-plugin-react": "^5.2.2",
    "jimp": "^0.2.21",
    "sharp": "^0.17.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "webpack": "^1.13.0"
  },
  "peerDependencies": {
    "jimp": "^0.2.21"
  }
}
